<!DOCTYPE html>
<html lang="pt-br">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>History</title>
    <link rel="icon" href="../../assets/Images/favicon.ico" />
    <link rel="stylesheet" href="../../css/History.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
    <script defer src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
        crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/FileSaver.js/2.0.0/FileSaver.min.js"
        integrity="sha512-csNcFYJniKjJxRWRV1R7fvnXrycHP6qDR21mgz1ZP55xY5d+aHLfo9/FcGDQLfn2IfngbAHd8LdfsagcCqgTcQ=="
        crossorigin="anonymous" referrerpolicy="no-referrer"></script>
</head>

<body>
    <header>
        <div class="detail"></div>
        <nav class="navbar navbar-expand-lg bg-body-primary shadow-sm ">
            <div class="container-fluid">
                <a class="navbar-brand" id="title">
                    <img src="../../assets/Images/logo.png" alt="IconeBosch" width="160px" height="36px" id="bosch_icon"
                        class="d-none d-md-flex" />
                </a>
                <div class="d-flex justify-content-end align-items-center">
                </div>
            </div>
            </div>
        </nav>
    </header>

    <div class="welcome-container">
        <div class="search_area">
            <form class="form-inline">
                <input id="searchInput" onkeyup="filterBySearch()" class="input_search" type="search"
                    placeholder="Pesquisar Processo" aria-label="Search">
                <button class="btn-search" type="button" onclick="filterBySearch()"><i class="bi bi-search"></i></button>
            </form>
        </div>

        <div class="card-area">
            <% for (const [key, value] of Object.entries(data)) { %>
                <div class="card" style="border-radius: 10px;" id="processCard">
                    <div class="status_process_area">
                        <div class="<%=
                        value.finished ? "status_off" : "status_on"
                      %>"></div>
                    </div>
                    <div class="card-body">
                        <h4 class="card-title">Processo: <%=value.code%></h4>
                        <h6 class="card-text mb-2">Data: <%=value.createdAt.toLocaleDateString("pt-br")%></h6>
                        <div class="card_btn_area mt-4">
                            <button type="button" class="card-btn editar" onclick="verDetalhes()">Ver
                                Detalhes</button>
                            <button type="button" class="card-btn apagar">Baixar</button>
                            <a href="./dashboard/<%= value.code %>" class="card-btn abrir">Abrir</a>
                        </div>
                    </div>
                </div>
            <%}%>
        </div>

    </div>

    <div class="content_form">
        <div id="excelPreview"></div>
    </div>
    
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="../../js/Excel.js"></script>
    <script src="../../js/SearchBtn.js"></script>
</body>

</html>